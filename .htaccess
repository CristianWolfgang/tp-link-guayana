
RewriteEngine On
RewriteRule ^search?q=([a-zA-Z0-9/]+)$ search.php?search=$1
RewriteRule ^company/([a-zA-Z0-9]+)$ search.php?company=$1
RewriteRule ^category/([a-zA-Z0-9]+)$ search.php?category=$1
RewriteRule ^promo search.php?promo
RewriteRule ^new search.php?new
RewriteRule ^product/([a-zA-Z0-9]+)$ myproduct.php?product=$1